<td>{{this.name}}</td>
<td>
    {{#if this.is_admin}}
        <i class="fa fa-fw fa-check-square-o"></i>
    {{else}}
        <i class="fa fa-fw fa-square-o"></i>
    {{/if}}
</td>
<td>
    {{this.jobs.length}}
</td>
<td>
    {{this.organizations.length}}
</td>
<td>
    <a data-userid="{{this.id}}" class="btn btn-warning btn-xs" type="button">
        <i class="fa fa-fw fa-pencil"></i>
        Edit
    </a>
</td>
<td>
    <a data-userid="{{this.id}}" class="btn btn-danger btn-xs" type="button">
        <i class="fa fa-fw fa-trash-o"></i>
        Delete
    </a>
</td>
